package rpictl.swing.frame;

import javax.swing.*;
import java.awt.*;

public class RpiCtl extends JFrame{
    public static void main(String[] args) {
        RpiCtl frame = new RpiCtl("FirstSwing");
        Container con = frame.getContentPane();
        con.setSize(300, 300);
        con.add(new JLabel("Hello Swing"));
        frame.setContentPane(con);
        frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        frame.pack();
        frame.setVisible(true);

    }

    /**
     * コンストラクタ
     * @param name フレームの名前
     */
    public RpiCtl(String name) {
        super(name);
    }
}
